{
  "development": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgres",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      },
      "keepAlive": true    
    }
  },
  "stage": {
    "dialect": "postgres",
		"use_env_variable" : "DATABASE_URL",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      },
      "keepAlive": true    
    }
  },
  "production": {
    "dialect": "postgres",
		"use_env_variable" : "DATABASE_URL",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      },
      "keepAlive": true    
    }
  }
}
